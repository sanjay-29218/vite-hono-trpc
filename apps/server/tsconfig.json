{
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "moduleResolution": "bundler",
    "module": "ESNext",
    "target": "ESNext",
    "types": ["node"],
    "skipLibCheck": true
  },
  "include": ["src/**/*"]
}
